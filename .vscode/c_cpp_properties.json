{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/**",
                "/usr/lib/x86_64-linux-gnu/glib-2.0/include/**",
                "${default}"
            ],
            "defines": [
                "${default}"
            ],
            "forcedInclude": [
                "${default}",
                "/usr/include/ctype.h"
            ],
            "compilerPath": "/usr/bin/x86_64-linux-gnu-gcc-11",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "/usr/include",
                    "/usr/lib/x86_64-linux-gnu/glib-2.0/include"
                ],
                "limitSymbolsToIncludedHeaders": false
            },
            "mergeConfigurations": true,
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}